{
  "base": "vendor/semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../../static/semantic/",
      "uncompressed": "../../static/semantic/components/",
      "compressed": "../../static/semantic/components/",
      "themes": "../../static/semantic/themes/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "rtl": "No",
  "components": [
    "reset",

    "breadcrumb",
    "button",
    "checkbox",
    "divider",
    "dropdown",
    "icon",
    "input",
    "label",
    "list",
    "segment",
    "transition"
  ],
  "version": "2.1.8"
}
